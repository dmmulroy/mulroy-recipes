{
  "name": "@mulroy-recipes/tanstack-frontend-worker",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "better-auth:gen": "npx @better-auth/cli@beta generate --config ./src/lib/auth/server.ts --output ./src/lib/db/auth-schema.ts",
    "build": "vite build && tsc --noEmit",
    "build:dev": "CLOUDFLARE_ENV=dev vite build && tsc --noEmit",
    "build:prod": "vite build && tsc --noEmit",
    "cf-typegen": "wrangler types",
    "d1:migrate:dev": "wrangler d1 migrations apply mulroy-recipes-d1-dev --env dev --remote",
    "d1:migrate:local": "wrangler d1 migrations apply mulroy-recipes-d1 --local",
    "d1:migrate:prod": "wrangler d1 migrations apply mulroy-recipes-d1 --remote",
    "d1:migrate:remote": "npm run d1:migrate:dev && npm run d1:migrate:prod",
    "deploy:dev": "npm run build:dev && wrangler deploy",
    "deploy:prod": "npm run build:prod && wrangler deploy",
    "dev": "vite --force",
    "drizzle:generate": "npx drizzle-kit generate --config ./src/lib/db/drizzle.config.ts",
    "postinstall": "npm run cf-typegen",
    "preview": "vite preview"
  },
  "dependencies": {
    "@livestore/adapter-web": "0.4.0-dev.16",
    "@livestore/livestore": "0.4.0-dev.16",
    "@livestore/peer-deps": "0.4.0-dev.16",
    "@livestore/react": "0.4.0-dev.16",
    "@livestore/sync-cf": "0.4.0-dev.16",
    "@livestore/wa-sqlite": "0.4.0-dev.16",
    "@overengineering/fps-meter": "0.2.4",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-router": "^1.134.13",
    "@tanstack/react-router-devtools": "^1.134.13",
    "@tanstack/react-start": "^1.134.14",
    "better-auth": "^1.3.34",
    "better-auth-cloudflare": "^0.2.8",
    "class-variance-authority": "^0.7.1",
    "cloudflare": "^5.2.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.0.0",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.34",
    "@cloudflare/vite-plugin": "^1.14.0",
    "@cloudflare/workers-types": "4.20251106.1",
    "@livestore/devtools-vite": "0.4.0-dev.16",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@vitejs/plugin-react": "^5.1.0",
    "better-sqlite3": "^12.4.1",
    "drizzle-kit": "^0.31.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.46.0"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
